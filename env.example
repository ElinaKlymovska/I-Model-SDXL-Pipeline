# RunPod API Access
RUNPOD_API_KEY=your_runpod_api_key_here

# RunPod Settings  
RUNPOD_GPU_TYPE=A40
RUNPOD_VOLUME_GB=50
RUNPOD_DISK_GB=40
RUNPOD_POD_ID=auto_detected

# WebUI API Configuration
WEBUI_API_URL=http://127.0.0.1:7860
WEBUI_PORT=3000

# Enhanced Face Correction Settings
DEFAULT_SDXL_MODEL=copax_realistic_xl
DEFAULT_DETAIL_MODEL=proteus_xl
DEFAULT_FACE_MODEL=face_yolov8m.pt
DEFAULT_CHARACTER_TYPE=female_portrait
DEFAULT_ENHANCEMENT_LEVEL=medium
DEFAULT_QUALITY_PRESET=balanced

# Model Presets
# basic: epicrealism_xl
# advanced: epicrealism_xl, copax_realistic_xl  
# professional: epicrealism_xl, copax_realistic_xl, proteus_xl, newreality_xl

# Face Detection Models
# face_yolov8n.pt - Fast
# face_yolov8s.pt - Balanced (recommended)
# face_yolov8m.pt - High accuracy
# face_yolov8l.pt - Maximum quality
# face_yolov8x.pt - Professional

# Pipeline Configuration
SAVE_INTERMEDIATE_RESULTS=True
SKIP_PREPROCESSING=False
SKIP_POSTPROCESSING=False

# Batch Processing
MAX_BATCH_SIZE=50
AUTO_PACKAGE_RESULTS=True

# Output Configuration
OUTPUT_FORMAT=jpg
OUTPUT_QUALITY=95

# GPU Settings (auto-detected)
FORCE_CPU=False
RTX5090_FORCE_GPU=False

# Logging
LOG_LEVEL=INFO
VERBOSE_LOGGING=False

# Optional: Custom model configurations
# CUSTOM_MODEL_PATH=/workspace/models/custom_model.safetensors