# Sensitive Configuration (API Keys, Secrets)
# НЕ КОМІТЬТЕ цей файл з реальними значеннями!

# RunPod API Access
RUNPOD_API_KEY=your_runpod_api_key_here

# Optional: RunPod Pod Information (auto-detected)
RUNPOD_POD_ID=auto_detected
RUNPOD_GPU_TYPE=A40
RUNPOD_VOLUME_GB=50
RUNPOD_DISK_GB=40

# WebUI API Configuration (if different from default)
WEBUI_API_URL=http://127.0.0.1:7860

# Optional: External Service API Keys
CIVITAI_API_KEY=your_civitai_api_key_if_needed
HUGGINGFACE_TOKEN=your_hf_token_if_needed

# Optional: Cloud Storage (if using external storage)
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret
S3_BUCKET_NAME=your_bucket_name

# Optional: Monitoring Services
WANDB_API_KEY=your_wandb_key_for_monitoring
TENSORBOARD_LOG_DIR=/workspace/logs

# Development Overrides (only for development)
# FORCE_CPU=false
# RTX5090_FORCE_GPU=false
# DEBUG_MODE=false